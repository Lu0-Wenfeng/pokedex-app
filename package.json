{
  "name": "pokedex-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rsuite/icons": "^1.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.1",
    "redux": "^4.2.0",
    "rsuite": "^5.83.2",
    "rxjs": "^7.8.2",
    "sass": "^1.89.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "rsbuild dev",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "quality": "npm run type-check && npm run lint && npm run format-check",
    "quality-fix": "npm run type-check && npm run lint-fix && npm run format"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rsbuild/core": "^1.4.11",
    "@rsbuild/plugin-less": "^1.3.1",
    "@rsbuild/plugin-react": "^1.3.4",
    "@rsbuild/plugin-sass": "^1.3.3",
    "@rsbuild/plugin-svgr": "^1.2.1",
    "@rsbuild/plugin-type-check": "^1.2.3",
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
