    "rxjs": "^7.5.7",
    "sass": "^1.55.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "rsbuild dev",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:debug": "jest --inspect-brk --runInBand --no-cache",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "quality": "npm run type-check && npm run lint && npm run format-check",
    "quality-fix": "npm run type-check && npm run lint-fix && npm run format",
    "quality-full": "npm run type-check && npm run lint && npm run format-check && npm run test:ci",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rsbuild/core": "^1.4.11",
    "@rsbuild/plugin-less": "^1.3.1",
    "@rsbuild/plugin-react": "^1.3.4",
    "@rsbuild/plugin-sass": "^1.3.3",
    "@rsbuild/plugin-svgr": "^1.2.1",
    "@rsbuild/plugin-type-check": "^1.2.3",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-onboarding": "^9.0.18",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.18",
    "eslint-plugin-testing-library": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-transform-css": "^6.0.3",
    "jest-transform-file": "^1.1.1",
    "jest-watch-typeahead": "^3.0.1",
    "msw": "^2.10.4",
    "prettier": "^3.6.2",
    "storybook": "^9.0.18",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  }
}
